[push]
  default = simple
[filter "media"]
  required = true
  clean = git media clean %f
  smudge = git media smudge %f
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[alias]
  sha = rev-parse --short HEAD
[commit]
  gpgsign = true
[gpg]
  program = /usr/local/bin/gpg2
