{
  "coc.preferences.bracketEnterImprove": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "ruby",
    "javascript",
    "typescript",
    "rust"
  ],
  "rust.build_on_save": true,
  "rust.clippy_preference": "on",
  "rust.all_features": true,
  "solargraph.folding":false,
  "solargraph.hover": true,
  "solargraph.diagnostics": true,
  "solargraph.autoformat": true,
  "solargraph.formatting": true,
  "codeLens.enable": true,
  "git.signOffset": 9999,
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact"],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "tsserver.formatOnType": true,
  "typescript.suggest.autoImports": true,
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  "explorer.keyMappings": {
    "k": "nodePrev",
    "j": "nodeNext",

    "*": "toggleSelection",
    "<tab>": "actionMenu",

    "h": "collapse",
    "l": "expand",
    "J": ["toggleSelection", "normal:j"],
    "K": ["toggleSelection", "normal:k"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "o": "expandOrCollapse",
    "<cr>": "open",
    "e": "open",
    "E": "openInVsplit",
    "t": "openInTab",
    "<bs>": "gotoParent",

    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",

    ".": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[[": "sourcePrev",
    "]]": "sourceNext",

    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",

    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  }
}
